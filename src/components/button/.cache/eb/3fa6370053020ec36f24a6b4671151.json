x;\n  letter-spacing: 1.2px;\n}\n\nh2 {\n  @include h2-text();\n  margin: 0;\n}\n\nh3 {\n  @include h3-text();\n  margin: 0;\n  text-transform: uppercase;\n}\n\nspan.body-text {\n  @include body-text();\n}\n","../../abstracts/_variables.scss":"$dark: #1f2041;\n$dark-75: rgba($dark, 0.75);\n$dark-50: rgba($dark, 0.5);\n$dark-25: rgba($dark, 0.25);\n$dark-10: rgba($dark, 0.1);\n$dark-5: rgba($dark, 0.05);\n\n\n$purple: #bc9cff;\n$blue:#8ba4f9;\n$green: #6fcf97;\n$gray:#e5e5e5;\n$white:#fff;\n$blue-light:#66d2ea;\n\n$purple-25: rgba($purple, 0.25);\n$blue-25: rgba($blue, 0.25);\n\n$font: Montserrat, Arial, sans-serif;\n$font-eng:Quicksand, Arial, sans-serif;\n\n\n","../../fonts.scss":"@import \"./abstracts/_variables\";\n@import \"./abstracts/_mixins\";\n\n@font-face {\n  font-family: Montserrat;\n  src: url(./assets/fonts/Montserrat-Regular.svg) format(\"svg\"),\n    url(./assets/fonts/Montserrat-Regular.ttf) format(\"ttf\"),\n    url(./assets/fonts/Montserrat-Regular.woff) format(\"woff\");\n  font-weight: 400;\n  font-style: normal;\n}\n\n@font-face {\n  font-family: Montserrat;\n  src: url(./assets/fonts/Montserrat-Bold.svg) format(\"svg\"),\n    url(./assets/fonts/Montserrat-Bold.ttf) format(\"ttf\"),\n    url(./assets/fonts/Montserrat-Bold.woff) format(\"woff\");\n  font-weight: 700;\n  font-style: normal;\n}\n\n@font-face {\n  font-family: Quicksand;\n  src: url(./assets/fonts/Quicksand-Regular.svg) format(\"svg\"),\n    url(./assets/fonts/Quicksand-Regular.ttf) format(\"ttf\"),\n    url(./assets/fonts/Quicksand-Regular.woff) format(\"woff\");\n  font-weight: 400;\n  font-style: normal;\n}\n\n@font-face {\n  font-family: Quicksand;\n  src: url(./assets/fonts/Quicksand-Bold.svg) format(\"svg\"),\n    url(./assets/fonts/Quicksand-Bold.ttf) format(\"ttf\"),\n    url(./assets/fonts/Quicksand-Bold.woff) format(\"woff\");\n  font-weight: 700;\n  font-style: normal;\n}\n\n.title {\n  display: block;\n  color: rgb(197, 15, 15);\n  padding: 50px;\n  font-family: \"MyriadPro-Regular\", \"Arial\", sans-serif;\n  font-size: 50px;\n}\n\n//компромис для canvas\n@font-face {\n  font-family: Montserrat-bold;\n  src: url(./assets/fonts/Montserrat-Bold.svg) format(\"svg\"),\n    url(./assets/fonts/Montserrat-Bold.ttf) format(\"ttf\"),\n    url(./assets/fonts/Montserrat-Bold.woff) format(\"woff\");\n  font-style: normal;\n}\n\n@font-face {\n  font-family: Quicksand-bold;\n  src: url(./assets/fonts/Quicksand-Bold.svg) format(\"svg\"),\n    url(./assets/fonts/Quicksand-Bold.ttf) format(\"ttf\"),\n    url(./assets/fonts/Quicksand-Bold.woff) format(\"woff\");\n  font-style: normal;\n}\n\nhtml {\n  font-size: 12px;\n  /* @media (max-width: 1959.98px) {\n    font-size: 8px;\n  }\n\n  @media (max-width: 1439.98px) {\n    font-size: 7px;\n  }\n\n  @media (max-width: 1199.98px) {\n    font-size: 12px;\n  }\n  @media (max-width: 1023.98px) {\n    font-size: 10px;\n  }\n\n  @media (max-width: 767.98px) {\n    font-size: 6.5px;\n  }\n\n  @media (max-width: 424.98px) {\n    font-size: 5px;\n  }\n\n  @media (max-width: 374.98px) {\n    font-size: 4px;\n  }\n\n  @media (max-width: 319.98px) {\n    font-size: 3px;\n  } */\n}\n\n/* body {\n  margin: 0;\n} */\n\n/* * {\n  outline: 1px solid gold;\n} */\n"},"lineCount":null}},"error":null,"hash":"6d76aff9281140ff8047bc14dcc06d0f","cacheData":{"env":{}}}