rrat, Arial, sans-serif;\n$font-eng:Quicksand, Arial, sans-serif;\n\n\n","nav.scss":"@import \"../../abstracts/_variables.scss\";\n@import \"../../abstracts/_mixins.scss\";\n\n.nav {\n  width: 530px;\n  @include body-text();\n  display: flex;\n  align-items: center;\n  line-height: 17px;\n  color: $dark-50;\n  text-transform: capitalize;\n  justify-content: center;\n  flex-shrink: 0;\n    \n  &__link {\n    transition: all 0.3s; \n    display: block;\n    box-sizing: border-box;\n    color: inherit;\n    text-decoration: none;\n    &::after {\n      @include body-text();\n      display: block;\n      content: attr(title);\n      height: 0px;\n      overflow: hidden;\n      visibility: hidden;\n      font-weight: normal;\n      line-height: 17px;\n      color: $dark-50;\n    }\n    &:hover {\n      font-weight: 700;\n    }\n  }\n\n  &__item {\n    text-align: center;\n    margin-right: 27px;\n    //\n    font-family: $font;\n    font-style: normal;\n    font-size: 14px;\n    line-height: 17px;\n    color: rgba(31, 32, 65, 0.5);\n    font-weight: normal;\n    font-style: normal;\n    position: relative;\n    cursor: pointer;\n    flex-shrink: 0;\n    list-style: none;\n    &:last-child {\n      margin-right: 0px; \n    }\n    &_selected {\n      color: rgba(31, 32, 65, 0.75);\n      font-family: $font;\n      font-style: normal;\n      font-weight: bold;\n      font-size: 14px;\n      line-height: 17px;\n    }\n  } \n\n  &__expanded {\n    cursor: pointer;\n    position: relative;\n    &_content {\n      display: none;\n      position: absolute;\n      border: 4px;\n      border-radius: 4px;\n      background-color: rgba(222,32,24,0.9);\n      box-shadow: 0px 18px 18px rgba(0,0,0,0.2);\n      padding: 10px 10px 10px 10px;\n      white-space: nowrap;\n      overflow: hidden;\n //     right: -15px;\n      width: 100%;\n      right: 0;\n      left: 0;\n      &-show {\n        display: block;  \n        align-items: center;\n      }\n    }\n    &_element {\n      color: white;\n      float: none;\n    }\n  }\n  &__arrow {\n    position: absolute;\n    right: -14px;\n    bottom: 3.8px;\n    &-active {\n      transform: rotate(180deg); \n    }\n  }\n}\n\nheader {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 30px 10%;\n}\n\n\n"},"lineCount":null}},"error":null,"hash":"f7b9168d56a3739a95191c0fb66290cf","cacheData":{"env":{}}}